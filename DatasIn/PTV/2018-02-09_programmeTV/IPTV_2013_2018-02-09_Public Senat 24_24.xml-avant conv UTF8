      <EMISSION CLEEMI="123576770" DATEMODIF="2018-01-04T16:40:27">
        <TITREEMISSION>Les dessous de la mondialisation</TITREEMISSION>
        <FORMAT CLE="5">Documentaire</FORMAT>
        <GENRE CLE="362" GENRESIMPLE="Documentaire de société">Découvertes</GENRE>
