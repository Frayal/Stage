        <TITREEMISSION>90' Enquêtes</TITREEMISSION>
        <FORMAT CLE="12">Magazine</FORMAT>
        <GENRE CLE="423" GENRESIMPLE="Magazine de société">Magazine de société</GENRE>
        <MAGAZINEXY>
          <COLLECTION CLE="12101">
            <TYPECOLLECTION CLE="19">Magazine / Titre</TYPECOLLECTION>
            <NOM>Magazine / 90' Enquêtes</NOM>
          </COLLECTION>
          <SOUSTITRE>Vacances d'été à petit prix : entre bons plans et arnaques</SOUSTITRE>
        </MAGAZINEXY>
        <LESPERSONNES>
          <PERSONNE CLE="107597">
            <FONCTION>Présentateur</FONCTION>
            <NOM>Rousseau</NOM>
            <PRENOM>Carole</PRENOM>
          </PERSONNE>
