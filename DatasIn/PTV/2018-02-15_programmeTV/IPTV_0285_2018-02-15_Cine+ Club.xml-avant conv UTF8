        <TITREEMISSION>Olivier Gourmet, exercice de style</TITREEMISSION>
        <FORMAT CLE="5">Documentaire</FORMAT>
        <GENRE CLE="357" GENRESIMPLE="Documentaire culturel">Cinéma</GENRE>
        <NATIONALITE CLE="348">
          <PAYS>France</PAYS>
          <LIBELLE>français</LIBELLE>
        </NATIONALITE>
        <DOCUMENTAIRE>
          <ANNEEREALISATION>2010</ANNEEREALISATION>
