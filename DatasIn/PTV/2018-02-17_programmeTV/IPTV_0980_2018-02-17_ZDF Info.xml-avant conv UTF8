    <DIFFUSION CLEDIF="171826347" DATEMODIF="2018-02-07T09:20:44">
      <DATEHEURE>2018-02-18T00:45:00</DATEHEURE>
      <DUREE>45</DUREE>
      <TITRE>ZDF-History</TITRE>
