        <LESPERSONNES>
          <PERSONNE CLE="96610">
            <FONCTION>Réalisateur</FONCTION>
            <NOM>Miské</NOM>
            <PRENOM>Karim</PRENOM>
          </PERSONNE>
          <PERSONNE CLE="1349934">
            <FONCTION>Réalisateur</FONCTION>
            <NOM>Ball</NOM>
            <PRENOM>Marc</PRENOM>
